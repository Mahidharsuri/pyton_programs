n = int(input("Enter a number to calculate its factorial: "))
factorial = 1

for i in range(1, n + 1):
    factorial *= i # 1=1*1

print("Factorial of", n, "is", factorial)

-------------------------------
Using math 
------------------------------- 
import math

def factorial(n):
    return(math.factorial(n))
num=6
print(factorial(num))
